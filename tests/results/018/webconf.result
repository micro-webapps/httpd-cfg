Listen 9090
<VirtualHost *:9090>
    ServerName localhost
    Alias /static "./my-static-dir"
    <Directory "./my-static-dir">
    </Directory>

</VirtualHost>

Listen 9443 ssl
<VirtualHost *:9443>
    ServerName localhost
    SSLEngine On
    SSLCertificateFile ./018/localhost.crt
    SSLCertificateKeyFile ./018/localhost.key
    Alias /static "./my-static-dir"
    <Directory "./my-static-dir">
    </Directory>

</VirtualHost>

